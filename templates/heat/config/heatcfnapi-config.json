{
  "command": "/usr/sbin/httpd -DFOREGROUND",
  "config_files": [
    {
      "source": "/var/lib/config-data/merged/heat.conf",
      "dest": "/etc/heat/heat.conf",
      "merge": false,
      "preserve_properties": true,
      "perm": "0644"
    },
    {
      "source": "/var/lib/config-data/merged/02-config.conf.conf",
      "dest": "/etc/heat/heat.conf.d/02-config.conf.conf",
      "merge": false,
      "preserve_properties": true,
      "perm": "0644"
    },
    {
      "source": "/var/lib/config-data/merged/03-config.conf.conf",
      "dest": "/etc/heat/heat.conf.d/03-config.conf.conf",
      "merge": false,
      "preserve_properties": true,
      "perm": "0644"
    },
    {
      "source": "/var/lib/config-data/merged/heat-cfnapi-httpd.conf",
      "dest": "/etc/httpd/conf/httpd.conf",
      "merge": false,
      "preserve_properties": true,
      "perm": "0644"
    }
  ],
  "permissions": [
    {
      "path": "/etc/heat/heat.conf.d",
      "owner": "heat:heat",
      "recurse": true
    },
    {
      "path": "/etc/httpd/",
      "owner": "apache:apache",
      "recurse": true
    },
    {
      "path": "/var/www/cgi-bin/heat",
      "owner": "heat:heat",
      "recurse": true
    }
  ]
}
